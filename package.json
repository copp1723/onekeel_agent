    "test-query-performance": "ts-node --esm src/scripts/test-query-performance.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate:imap-filters": "node migrations/update-imap-filters-table.js",
    "migrate:failed-emails": "node migrations/create-failed-emails-table.js",
    "health-check:imap": "node scripts/run-imap-health-check.js",
    "start:health-checks": "ts-node --esm src/scripts/start-health-checks.ts",
    "check-duplicates": "npx jscpd .",
    "start:monitoring": "ts-node --esm src/scripts/start-monitoring.ts",
    "run:health-checks": "ts-node --esm src/scripts/run-health-checks.ts",
    "prepare": "husky install",
    "commit": "git-cz"
